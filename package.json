{
  "name": "be-nc-news",
  "version": "1.0.0",
  "description": "bc-nc-news",
  "main": "index.js",
  "scripts": {
    "migrate-make": "knex migrate:make",
    "migrate-rollback": "knex migrate:rollback",
    "migrate-latest": "knex migrate:latest && npm run query-db",
    "query-db": "psql -f ./db/query-db.sql > query-output.txt",
    "setup-dbs": "psql -f db/setup.sql",
    "seed-test": "npm run setup-dbs && NODE_ENV=test knex seed:run",
    "seed": "npm run migrate-rollback && npm run migrate-latest && knex seed:run",
    "test-utils": "jest spec/utils.spec.js",
    "test": "jest spec/app.spec.js && npm run query-db"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northcoders/be-nc-news.git"
  },
