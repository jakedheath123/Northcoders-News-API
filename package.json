{
  "name": "be-nc-news",
  "version": "1.0.0",
  "description": "bc-nc-news",
  "main": "index.js",
  "scripts": {
    "migrate-make": "knex migrate:make",
    "migrate-rollback": "knex migrate:rollback",
    "migrate-latest": "knex migrate:latest && npm run query-db",
    "query-db": "psql -f ./db/query-db.sql > query-output.txt",
    "setup-dbs": "psql -f db/setup.sql",
    "seed": "npm run setup-dbs && knex seed:run",
    "test-utils": "jest spec/utils.spec.js",
    "test": "jest spec/app.spec.js && npm run query-db",
    "dev": "nodemon listen.js",
    "seed:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex seed:run",
    "migrate-latest:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:latest",
    "migrate-rollback:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:rollback",
    "start": "node listen.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northcoders/be-nc-news.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/northcoders/be-nc-news/issues"
  },
  "homepage": "https://github.com/northcoders/be-nc-news#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "chai-sorted": "^0.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jest": "^25.1.0",
    "knex": "^0.20.10",
    "mocha": "^7.1.0",
    "pg": "^7.18.2",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.2"
  }
}
